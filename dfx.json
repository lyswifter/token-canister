{
  "canisters": {
    "token_canister": {
      "build": "cargo build --target wasm32-unknown-unknown --package token_canister --release",
      "candid": "src/token_canister/src/token_canister.did",
      "wasm": "target/wasm32-unknown-unknown/release/token_canister.wasm",
      "type": "custom"
    },
    "token_canister_assets": {
      "dependencies": [
        "token_canister"
      ],
      "frontend": {
        "entrypoint": "src/token_canister_assets/src/index.html"
      },
      "source": [
        "src/token_canister_assets/assets",
        "dist/token_canister_assets/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}